{
  "name": "snp-gulp-tasks",
  "version": "0.1.5",
  "description": "Salt & Pepper gulp tasks set",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint",
    "lint": "coffeelint ./*.coffee ./tasks ./plugins ./lib",
    "precommit": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/snphq/snp-gulp-tasks.git"
  },
  "keywords": [
    "snp",
    "gulp",
    "generator-sp",
    "tasks",
    "build"
  ],
  "author": "Maksim Koretskiy <maksim.koretskiy@saltpepper.ru>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/snphq/snp-gulp-tasks/issues"
  },
  "homepage": "https://github.com/snphq/snp-gulp-tasks",
  "dependencies": {
    "async": "^1.5.2",
    "autoprefixer": "^6.2.3",
    "browser-sync": "^2.9.1",
    "chokidar": "^1.0.1",
    "coffee-script": "^1.9.0",
    "csswring": "^4.1.1",
    "del": "^2.2.0",
    "gulp": "^3.8.10",
    "gulp-bless": "^3.0.1",
    "gulp-cache": "^0.4.1",
    "gulp-cached": "^1.0.1",
    "gulp-coffee": "^2.2.1",
    "gulp-coffeelint": "^0.6.0",
    "gulp-concat": "^2.4.1",
    "gulp-css-image": "^0.2.1",
    "gulp-filter": "^3.0.1",
    "gulp-flatten": "0.2.0",
    "gulp-gzip": "^1.1.0",
    "gulp-if": "^2.0.0",
    "gulp-ignore": "^2.0.1",
    "gulp-image-preload": "~0.0.6",
    "gulp-jade": "^1.0.0",
    "gulp-livereload": "^3.6.0",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.0.0",
    "gulp-postcss": "^6.0.1",
    "gulp-preprocess": "^2.0.0",
    "gulp-rename": "^1.2.0",
    "gulp-rev-all": "^0.8.18",
    "gulp-sass": "^2.1.1",
    "gulp-size": "^2.0.0",
    "gulp-sourcemaps": "^1.2.8",
    "gulp-sync": "^0.1.4",
    "gulp-uglify": "~1.5.1",
    "gulp-util": "^3.0.1",
    "gulp.spritesmith": "^6.0.1",
    "http-proxy": "^1.8.1",
    "imagemin-jpegoptim": "^4.0.0",
    "imagemin-optipng": "^4.2.0",
    "imagemin-pngquant": "^4.0.0",
    "jade": "^1.8.2",
    "jstransformer-coffee-script": "^1.0.0",
    "lodash": "~3.10.1",
    "merge-stream": "^1.0.0",
    "node-notifier": "^4.1.0",
    "node-sass": "^3.4.2",
    "postcss": "5.0.14",
    "postcss-import": "^7.1.3",
    "postcss-url": "^5.0.2",
    "requirejs": "^2.1.15",
    "rsvp": "^3.0.18",
    "through2": "^2.0.0",
    "urldata": "0.0.1",
    "vinyl-paths": "^2.1.0",
    "webpack": "^1.12.11",
    "webpack-dev-server": "^1.14.1"
  },
  "devDependencies": {
    "coffeelint": "^1.14.2",
    "husky": "^0.10.2"
  },
  "coffeelintConfig": {
    "coffeescript_error": {
      "level": "error"
    },
    "arrow_spacing": {
      "name": "arrow_spacing",
      "level": "ignore"
    },
    "no_tabs": {
      "name": "no_tabs",
      "level": "error"
    },
    "no_trailing_whitespace": {
      "name": "no_trailing_whitespace",
      "level": "warn",
      "allowed_in_comments": false,
      "allowed_in_empty_lines": true
    },
    "max_line_length": {
      "name": "max_line_length",
      "value": 120,
      "level": "warn",
      "limitComments": true
    },
    "line_endings": {
      "name": "line_endings",
      "level": "ignore",
      "value": "unix"
    },
    "no_trailing_semicolons": {
      "name": "no_trailing_semicolons",
      "level": "error"
    },
    "indentation": {
      "name": "indentation",
      "value": 2,
      "level": "warn"
    },
    "camel_case_classes": {
      "name": "camel_case_classes",
      "level": "error"
    },
    "colon_assignment_spacing": {
      "name": "colon_assignment_spacing",
      "level": "warn",
      "spacing": {
        "left": 0,
        "right": 1
      }
    },
    "no_implicit_braces": {
      "name": "no_implicit_braces",
      "level": "ignore",
      "strict": false
    },
    "no_plusplus": {
      "name": "no_plusplus",
      "level": "ignore"
    },
    "no_throwing_strings": {
      "name": "no_throwing_strings",
      "level": "error"
    },
    "no_backticks": {
      "name": "no_backticks",
      "level": "error"
    },
    "no_implicit_parens": {
      "name": "no_implicit_parens",
      "level": "ignore"
    },
    "no_empty_param_list": {
      "name": "no_empty_param_list",
      "level": "warn"
    },
    "no_stand_alone_at": {
      "name": "no_stand_alone_at",
      "level": "warn"
    },
    "space_operators": {
      "name": "space_operators",
      "level": "ignore"
    },
    "duplicate_key": {
      "name": "duplicate_key",
      "level": "error"
    },
    "empty_constructor_needs_parens": {
      "name": "empty_constructor_needs_parens",
      "level": "ignore"
    },
    "cyclomatic_complexity": {
      "name": "cyclomatic_complexity",
      "value": 30,
      "level": "warn"
    },
    "newlines_after_classes": {
      "name": "newlines_after_classes",
      "value": 3,
      "level": "warn"
    },
    "no_unnecessary_fat_arrows": {
      "name": "no_unnecessary_fat_arrows",
      "level": "warn"
    },
    "missing_fat_arrows": {
      "name": "missing_fat_arrows",
      "level": "ignore"
    },
    "non_empty_constructor_needs_parens": {
      "name": "non_empty_constructor_needs_parens",
      "level": "ignore"
    },
    "no_unnecessary_double_quotes": {
      "name": "no_unnecessary_double_quotes",
      "level": "warn"
    },
    "spacing_after_comma": {
      "name": "spacing_after_comma",
      "level": "warn"
    }
  }
}
